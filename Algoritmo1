<?php

print "digite o primeiro valor:";
   $primeiro = fgets(STDIN);

print "digite o segundo valor: ";
    $segundo = fgets(STDIN);

if ($primeiro > $segundo){
    print "o primeiro valor é maior: $primeiro \n";
}
elseif ($primeiro == $segundo){
    print "os valores são iguais \n";
}
else {
    print "o segundo valor é maior: $segundo \n";
}
